    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-validator": "^7.0.1",
    "joi": "^17.10.1",
    "jsonwebtoken": "^9.0.2",
    "kafkajs": "^2.2.4",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "sharp": "^0.33.4",
    "tslog": "^4.9.2",
    "typedi": "^0.10.0",
    "typeorm": "^0.3.17"
  },
