  "description": "User Service",
  "author": "Anton Rybalko",
  "license": "UNLICENSED",
  "private": true,
  "main": "presentation/web/serverListener.js",
  "scripts": {
    "build": "tsc --build",
    "dev": "nodemon --config nodemon.json src/presentation/web/serverListener.ts",
    "cli": "ts-node src/presentation/cli/cli.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:run": "rm -rf dist/src/adapter/persistence/migration/* && yarn build && yarn typeorm migration:run -- -d src/adapter/persistence/data-source.ts",
    "migration:run:dev": "rm -rf dist/src/adapter/persistence/migration/* && yarn build && NODE_ENV=development yarn typeorm migration:run -- -d src/adapter/persistence/data-source.ts",
    "migration:revert": "yarn build && yarn typeorm migration:revert -- -d src/adapter/persistence/data-source.ts",
    "migration:revert:dev": "yarn build && NODE_ENV=development yarn typeorm migration:revert -- -d src/adapter/persistence/data-source.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "test": "jest",
    "test:unit": "jest --config jest.unit.config.js",
    "test:integration": "jest --config jest.config.js",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.6.0",
    "@types/pg": "^8.10.2",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.3",
    "prettier": "^3.0.3",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.616.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/kafkajs": "^1.9.0",
    "@types/morgan": "^1.9.5",
    "bcrypt": "^5.1.1",
    "class-sanitizer": "^1.0.1",
